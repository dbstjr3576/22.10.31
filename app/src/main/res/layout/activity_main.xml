<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="인공지능학과"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/button" android:textSize="34sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="4dp" android:layout_marginStart="4dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이윤석"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/textView2" android:textSize="34sp"
            app:layout_constraintHorizontal_bias="0.498"/>
    <TextView
            android:text="202202494"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView"
            app:layout_constraintTop_toTopOf="@+id/button" app:layout_constraintStart_toStartOf="@+id/button"
            android:layout_marginTop="264dp" android:layout_marginStart="12dp" android:textSize="34sp"/>

</androidx.constraintlayout.widget.ConstraintLayout>